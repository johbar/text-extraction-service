{
	"image": "mcr.microsoft.com/devcontainers/go:1.23",
	"features": {
		"ghcr.io/devcontainers/features/sshd:1": {}
	},
	"remoteUser": "vscode",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go"
			],
			"settings": {
				"go.toolsManagement.checkForUpdates": "local",
				"go.useLanguageServer": true,
				"go.gopath": "/go"
			}
		}
	}
}